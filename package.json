    "@nx/js": "17.2.8",
    "@nx/vite": "17.2.8",
    "@nx/workspace": "17.2.8",
    "@svitejs/changesets-changelog-github-compact": "^1.1.0",
    "@swc-node/register": "~1.6.7",
    "@swc/core": "~1.3.85",
    "@types/node": "18.16.9",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "@vitest/coverage-v8": "~0.34.6",
    "@vitest/ui": "~0.34.6",
    "eslint": "~8.48.0",
