{
  "name": "@test-release/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "changeset": "changeset",
    "release.prepare": "nx run-many --target=build --all --parallel",
    "release.setroot": "mv pnpm-workspace.yaml pnpm-workspace.yaml-backup && cp pnpm-workspace-for-release.yaml pnpm-workspace.yaml",
    "release": "pnpm release.prepare && release.setroot && release.publish && release.resetroot",
    "release.publish": "nx changeset publish",
    "release.resetroot": "mv pnpm-workspace.yaml-backup pnpm-workspace.yaml"
  },
  "private": true,
  "dependencies": {
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.0",
